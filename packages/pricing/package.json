{
  "name": "@speedy-van/pricing",
  "version": "1.0.0",
  "description": "Pricing engine and calculation logic for Speedy Van",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest"
  },
  "dependencies": {
    "@speedy-van/shared": "workspace:*",
    "@speedy-van/utils": "workspace:*"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "typescript": "^5.3.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./calculator": {
      "types": "./dist/calculator/index.d.ts",
      "default": "./dist/calculator/index.js"
    },
    "./rules": {
      "types": "./dist/rules/index.d.ts",
      "default": "./dist/rules/index.js"
    }
  }
}
